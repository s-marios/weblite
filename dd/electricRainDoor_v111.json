        "type": "string",
        "enum": [
          "obstacleCaught",
          "outageRecovery",
          "timeOut",
          "batteryLow"
        ],
        "values": [
          {
            "value": "obstacleCaught",
            "descriptions": {
              "ja": "障害物挟込み",
